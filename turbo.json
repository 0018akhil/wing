{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "compile": {
      "dependsOn": ["^compile"],
      "outputs": ["dist/**"]
    },
    "bundle": {
      "dependsOn": ["compile", "^bundle"],
      "cache": false
    },
    "preview": {
      "dependsOn": ["^compile"],
      "cache": false
    },
    "dev": {
      "dependsOn": ["^compile"],
      "persistent": true,
      "cache": false
    },
    "eslint": {
      "outputs": []
    },
    "tsc": {
      "dependsOn": ["compile"],
      "outputs": []
    },
    "vitest": {
      "outputs": ["coverage/**"]
    },
    "vitest:update": {
      "cache": false
    },
    "playwright": {
      "dependsOn": ["compile"],
      "outputs": ["playwright-report/**"]
    },
    "playwright:update": {
      "dependsOn": ["compile"],
      "cache": false
    }
  }
}
